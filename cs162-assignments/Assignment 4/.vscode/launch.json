{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"miDebuggerPath": "/cygwin64/bin/gdb.exe",
			"program": "${workspaceRoot}/game.exe",
			"args": ["12", "12"],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": true,
			"MIMode": "gdb",
			"setupCommands": [
			    {
			        "description": "Enable pretty-printing for gdb",
			        "text": "-enable-pretty-printing",
			        "ignoreFailures": true
			    }
			]
		}
	]
}